Maegdefessel, Lars and Schunkert, Heribert and Sager, Hendrik B
and Kessler, Thorsten},
 copyright = {https://creativecommons.org/licenses/by/4.0},
 journal = {Basic Res. Cardiol.},
 keywords = {Atherosclerosis; Coronary artery disease; Genetics; SVEP1},
 language = {en},
 month = {November},
 number = {6},
 pages = {67},
 publisher = {Springer Science and Business Media LLC},
 title = {Functional investigation of the coronary artery disease gene
SVEP1},
 volume = {115},
 year = {2020}
}

